{
	"name": "jeroen/json-dump-reader",
	"description": "Library that provides ways to read from and iterate through the entities of a Wikibase JSON dump",
	"keywords": [
		"Wikibase",
		"Wikidata",
		"Dump",
		"JSON"
	],
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "Jeroen De Dauw",
			"homepage": "http://jeroendedauw.com"
		}
	],
	"require": {
		"php": ">=5.4.0",

		"wikibase/data-model": "~2.0",
		"wikibase/data-model-serialization": "~1.1",

		"jeroen/batching-iterator": "~2.1"
	},
	"require-dev": {
		"data-values/iri": "~0.1",
		"data-values/number": "~0.1",
		"data-values/geo": "~1.0",
		"data-values/time": "~0.6",

		"jeroen/json-dump-data": "~0.1"
	},
	"autoload": {
		"psr-4": {
			"Wikibase\\JsonDumpReader\\": "src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.1.x-dev"
		}
	}
}
